<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zhaoyuncapital.mapper.UserInfoMapper">

    <select id="username" resultType="com.zhaoyuncapital.model.UserInfo">
        select * from user_info where user_info.username=#{username};
    </select>
    <select id="findUserInfo" resultType="com.zhaoyuncapital.model.UserInfo">
        select * from user_info
    </select>
</mapper>